<Project Sdk="Godot.NET.Sdk/3.3.0">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <DefaultItemExcludes>Generated\**</DefaultItemExcludes>
    <DefaultItemExcludes>SimpleEcs\**</DefaultItemExcludes>
    <DefaultItemExcludes>SimpleEcsTest\**</DefaultItemExcludes>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Generated\**" />
    <Compile Remove="SimpleEcs\**" />
    <Compile Remove="SimpleEcsTest\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Generated\**" />
    <EmbeddedResource Remove="SimpleEcs\**" />
    <EmbeddedResource Remove="SimpleEcsTest\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="Generated\Generated.csproj" 
      OutputItemType="Analyzer"
      ReferenceOutputAssembly="false"/>
    <ProjectReference Include="SimpleEcs\SimpleEcs.csproj" />
  </ItemGroup>
    <ItemGroup>
  <Reference Include="Loyc.Collections">
      <HintPath>Loyc.Collections.dll</HintPath>
  </Reference>
  <Reference Include="Loyc.Interfaces">
      <HintPath>Loyc.Interfaces.dll</HintPath>
  </Reference>
  <Reference Include="Loyc.Essentials">
      <HintPath>Loyc.Essentials.dll</HintPath>
  </Reference>
  </ItemGroup>
</Project>
